<UserControl x:Class="Netsy.WpfUI.Controls.FrontListings"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>

        <StackPanel Orientation="Horizontal" Margin="1">
            <Button Content="Get Front Items" Margin="0,0,1,0" CommandParameter="{Binding}" />
            <Button Content="First" Margin="0,0,1,0" CommandParameter="{Binding}" />
            <Button Content="Previous" Margin="0,0,1,0" CommandParameter="{Binding}" />
            <Button Content="Next" Margin="0" CommandParameter="{Binding}" />
        </StackPanel>

    </Grid>
</UserControl>
