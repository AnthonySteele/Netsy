<Window x:Class="Netsy.WpfUI.Windows.Main.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Controls="clr-namespace:Netsy.WpfUI.Controls"
    Title="Netsy" Height="400" Width="500">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <TabControl>
            <TabItem Header="_Front Items">
                <Controls:FrontListings  DataContext="{Binding FrontFeaturedListings}" />
            </TabItem>
            <TabItem Header="_Other stuff">
            </TabItem>
        </TabControl>
    </Grid>
</Window>
